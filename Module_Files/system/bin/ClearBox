                  if am start -a android.intent.action.VIEW -d "https://yhfx.jwznb.com/share?key=yigOTedUjh62&ts=1747355950" >>/dev/null; then
                      echo " Â» è·³è½¬æˆåŠŸï¼"
                  else
                      echo " Â» è·³è½¬å¤±è´¥ï¼"
                  fi
                  ;;
                3)
                  if am start -a android.intent.action.VIEW -d "https://t.me/clearboxmodule" >>/dev/null; then
                      echo " Â» è·³è½¬æˆåŠŸï¼"
                  else
                      echo " Â» è·³è½¬å¤±è´¥ï¼"
                  fi
                  ;;
                *)
                  echo -en "\033[91m Â» è¾“å…¥é”™è¯¯ï¼ï¼æ­£åœ¨è¿”å›ä¸»é¡µï¼\033[0m"
                  ;;
            esac
            ;;
          01)
            clear
            echo -e "\033[104m [é…ç½®ç®¡ç†]\033[0m"
            echo -e "\033[96m ==============================================\033[0m\n"
            echo -e "\033[93m \t1ï¼šå¤‡ä»½\033[0m\n"
            echo -e "\033[93m \t2ï¼šæ¢å¤\033[0m\n"
            echo -e "\033[96m ==============================================\033[0m\n"
            echo -en " è¯·è¾“å…¥ç›¸åº”åºå·:"
            read backup_option_input
            case "$backup_option_input" in
                1)
                  clear
                  "$home_dir/BashCore" configManager backup
                  ;;
                2)
                  clear
                  echo -en " Â» è¯·è¾“å…¥é…ç½®æ–‡ä»¶å®Œæ•´è·¯å¾„: "
                  read configFile
                  "$home_dir/BashCore" configManager recovery "$configFile"
                  ;;
                *)
                  echo -en "\033[91m Â» è¾“å…¥é”™è¯¯ï¼ï¼æ­£åœ¨è¿”å›ä¸»é¡µï¼\033[0m"
                  ;;
            esac
            ;;
          02)
            echo -en " Â» ç¡®å®šå®Œå…¨å¸è½½ ClearBox å¹¶æ¸…ç†æ®‹ç•™ğŸ˜‰ï¼Ÿ(y/n): "
            read ok_input
            case "$ok_input" in
                y | Y)
                  sh "$home_dir/uninstall.sh"
                  echo " Â» è°¢è°¢æ‚¨ä¸€ç›´ä»¥æ¥çš„æ”¯æŒï¼Œæ¬¢è¿å†æ¥â¤ï¸â¤ï¸"
                  echo " Â» Byeï¼"
                  clear && exit 0
                  ;;
                *)
                  echo -en "\033[92m Â» æ‚¨é€‰æ‹©äº†å¦ï¼æ­£åœ¨è¿”å›ä¸»é¡µï¼\033[0m"
                  ;;
            esac
            ;;
          03)
            clear
            echo -e "\033[104m [æ—¥å¿—]\033[0m"
            if [ -f "$work_dir/LOG.log" ]; then
                more "$work_dir/LOG.log"
            fi
            echo -en "\033[92m Â» å›è½¦è¿”å›\033[0m"; read null
            ;;
          *)
            echo -en "\033[91m Â» è¾“å…¥é”™è¯¯ï¼ï¼æ­£åœ¨è¿”å›ä¸»é¡µï¼\033[0m"
            ;;
        esac
        echo -en "\033[92m Â» å›è½¦è¿”å›\033[0m"; read null
        ;;
    e | E | q | Q)
      clear
      echo " Â» Byeï¼æ¬¢è¿å†æ¥â¤ï¸â¤ï¸"
      exit 0
      ;;
    *)
      echo -en "\033[92m Â» å·²ç»æ˜¯æœ€åä¸€å±‚äº†å‘¦ï¼Œé”®å…¥Eæˆ–Qé€€å‡ºï¼\033[0m"
      ;;
esac
}

