                *)
                  echo -en "\033[92m Â» æ‚¨é€‰æ‹©äº†å¦ï¼æ­£åœ¨è¿”å›ä¸»é¡µï¼\033[0m"
                  ;;
            esac
            ;;
          03)
            clear
            echo -e "\033[104m [æ—¥å¿—]\033[0m"
            if [ -f "$work_dir/LOG.log" ]; then
                more "$work_dir/LOG.log"
            fi
            echo -en " Â» å›è½¦è¿”å›"
            read null
            ;;
          *)
            echo -en "\033[91m Â» è¾“å…¥é”™è¯¯ï¼ï¼æ­£åœ¨è¿”å›ä¸»é¡µï¼\033[0m"
            ;;
        esac
        ;;
    e | E | q | Q)
      clear
      echo " Â» Byeï¼æ¬¢è¿å†æ¥â¤ï¸â¤ï¸"
      exit 0
      ;;
    *)
      echo -en "\033[92m Â» å·²ç»æ˜¯æœ€åä¸€å±‚äº†å‘¦ï¼Œé”®å…¥Eæˆ–Qé€€å‡ºï¼\033[0m"
      ;;
esac
}

